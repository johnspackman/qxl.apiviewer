{
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
  "environment": {
    "qx.icontheme": "Tango",
    "qxl.apiviewer.exclude": [
      "qxl.*",
      "q.*"
    ]
  },
  "ignores": [
    "require",
    "process",
    "loadSass",
    "__dirname"
  ],
  "applications": [
    {
      "class": "qxl.apiviewer.Application",
      "theme": "qxl.apiviewer.Theme",
      "name": "apiviewer",
      "title": "Qooxdoo API Viewer",
      "bootPath": "source/boot"
    }
  ],
  "include": [
    "qx.*",
    "q",
    "qxWeb"
  ],
  "exclude": [
    "qx.tool.*",
    "qx.module.Blocker",
    "qx.module.Placement"
  ],
  "targets": [
    {
      "type": "source",
      "outputPath": "compiled/source",
      "bundle": {
        "include": [
          "qx.*"
        ]
      },
      "babelOptions": {
        "targets": "edge >=18, chrome >= 73, firefox >= 66"
      }
    },
    {
      "type": "build",
      "outputPath": "compiled/build",
      "deployPath": "html",
      "babelOptions": {
        "targets": "edge >=18, chrome >= 73, firefox >= 66"
      }
    }
  ],
  "defaultTarget": "source",
  "locales": [
    "en"
  ],
  "serve": {
    "listenPort": 8080
  },
  "sass": {
    "compiler": "legacy"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": "2020"
    }
  }
}
